@page "/"

@layout MainLayout

<PageTitle>Home</PageTitle>

<h1>Hello, world!</h1>

<div>
    @if (LastMessage is not null)
    {
        <p>@LastMessage</p>
    }

    @if (Messages is not null)
    {
        @foreach (string message in Messages)
        {
            <p>@message</p>
        }
    }
</div>